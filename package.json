{
    "name": "club-event-subscription",
    "version": "1.0.0",
    "description": "Register to a club event",
    "main": "index.js",
    "scripts": {
        "start:int": "yarn build && docker-compose -f docker-compose.composer.yml up composer && docker-compose up php",
        "build": "NODE_ENV=production webpack --config webpack.config.js",
        "inspect": "NODE_ENV=inspect webpack --config webpack.config.js",
        "start": "webpack-dev-server --config webpack.config.js",
        "test": "TZ=Europe/Paris jest",
        "test:e2e": "sh e2e/e2e.sh",
        "e2e": "jest -c e2e/jest.e2e.config.js",
        "lint": "npm-run-all lint:*",
        "lint:eslint": "eslint --ext .js,.ts,.tsx .",
        "lint:format": "prettier --config ./.prettierrc --check \"{,!(target|coverage|node_modules)/**/,.}/*{.ts,.tsx,.js,.jsx,.json,.css,.scss,.md,.php,.html}\"",
        "validate": "npm-run-all lint:* test build test:e2e",
        "format": "npm-run-all format:*",
        "format:prettier": "prettier --config ./.prettierrc --write \"{,!(target|coverage|node_modules)/**/,.}/*{.ts,.tsx,.js,.jsx,.json,.css,.scss,.md,.php,.html}\""
    },
